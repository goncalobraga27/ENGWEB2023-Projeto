        script.
            //pesquisa_rapida
            document.getElementById('pesquisa_rapida').addEventListener('keyup', function(event) {
                if (event.keyCode === 13) {
                    event.preventDefault();
                    var input = document.getElementById('pesquisa_rapida');
                    var p = input.value;
                    window.location.href = '/pesquisaRapida/' + p;
                }
            });