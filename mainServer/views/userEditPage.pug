extends layout

block content
  .w3-card-4
    header.w3-container.w3-purple
        h2 User Edit Form
    form.w3-container(method="POST")
        fieldset
            label Username 
            input.w3-input.w3-round(type="text" name="username" readonly value=u)
            label Name
            input.w3-input.w3-round(type="text" name="name")
            label Level
            input.w3-input.w3-round(type="text" name="level")               
            button.w3-btn.w3-purple.w3-mb-2(type="submit") Register
            button.w3-btn.w3-purple.w3-mb-2
              a(href="/adminUsers") Return

    footer.w3-container.w3-purple
        h5 Generated by TP_EngWeb2023 in #{d} 